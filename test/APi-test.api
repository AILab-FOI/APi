// Import holona

import ivek


// definicija kanala
channel bla :
	{ ?x : 123.321, '1':2 } -> { 'var' : ?x }

channel juhu :
	{ ?yaaa : "123.321", '1':{ 'a':2 } } -> { 'var' : ?yaaa }


channel external.

channel regexx :
	regex '(?P<bla>ggu)' -> json { 'var' : ?x }

channel lxml :
	xml <proba x="3">a</proba> -> json { 'var' : ?x }

channel xmlvar :
	xml <proba ?x="3" y=?zaaa>3aanna 4banana ?gaga 5huhu đšćč 7876ashbdukh<?a /></proba> -> json { 'var' : ?x }


// definicija agenta
agent a ( x y z ) :
	x -> self -> z
	self -> y
	z -> c -> 0

agent b :
	self -> a


// pokretanje agenata

start a b c

start ( a b ) | c

start a b | c

start a+ | b c

start a ! b | c d

start a & b

// komunikacija s okružjem (drugim holonima ili game engine-ovima)

environment :
      input1 <-> { 'val1': ?x, 'val2': ?y }
      output1 <-> { 'action': ?act }


